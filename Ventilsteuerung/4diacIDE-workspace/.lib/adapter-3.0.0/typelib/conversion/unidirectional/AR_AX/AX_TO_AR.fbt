<?xml version="1.0" encoding="UTF-8"?>
<FBType Name="AX_TO_AR" Comment="Composite FB for converting AX (BOOL) adapter to AR (REAL) adapter using F_SEL">
	<Identification Standard="61499-2" Description="Copyright (c) 2026 HR Agrartechnik GmbH  &#10; &#10;This program and the accompanying materials are made  &#10;available under the terms of the Eclipse Public License 2.0  &#10;which is available at https://www.eclipse.org/legal/epl-2.0/  &#10; &#10;SPDX-License-Identifier: EPL-2.0">
	</Identification>
	<VersionInfo Organization="HR Agrartechnik GmbH" Version="1.0" Author="Franz Hoepfinger" Date="2026-02-17">
	</VersionInfo>
	<CompilerInfo packageName="adapter::conversion::unidirectional">
		<Import declaration="iec61131::selection::F_SEL"/>
	</CompilerInfo>
	<InterfaceList>
		<Plugs>
			<AdapterDeclaration Name="AR_OUT" Type="adapter::types::unidirectional::AR" Comment="REAL Adapter Output" x="2000" y="0"/>
		</Plugs>
		<Sockets>
			<AdapterDeclaration Name="AX_IN" Type="adapter::types::unidirectional::AX" Comment="BOOL Adapter Input" x="0" y="0"/>
		</Sockets>
	</InterfaceList>
	<FBNetwork>
		<FB Name="Selection" Type="iec61131::selection::F_SEL" x="1000" y="0">
			<Parameter Name="IN0" Value="REAL#0.0"/>
			<Parameter Name="IN1" Value="REAL#1.0"/>
		</FB>
		<EventConnections>
			<Connection Source="AX_IN.E1" Destination="Selection.REQ" dx1="100"/>
			<Connection Source="Selection.CNF" Destination="AR_OUT.E1" dx1="100"/>
		</EventConnections>
		<DataConnections>
			<Connection Source="AX_IN.D1" Destination="Selection.G" dx1="100"/>
			<Connection Source="Selection.OUT" Destination="AR_OUT.D1" dx1="100"/>
		</DataConnections>
	</FBNetwork>
	<Attribute Name="eclipse4diac::core::TypeHash" Value="''"/>
</FBType>
