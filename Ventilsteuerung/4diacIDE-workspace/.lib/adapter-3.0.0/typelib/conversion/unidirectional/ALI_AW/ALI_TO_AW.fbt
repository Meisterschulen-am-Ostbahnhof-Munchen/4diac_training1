	</Identification>
	<VersionInfo Organization="HR Agrartechnik GmbH" Version="1.0" Author="Franz Hoepfinger" Date="2026-02-17">
	</VersionInfo>
	<CompilerInfo packageName="adapter::conversion::unidirectional">
		<Import declaration="iec61131::conversion::F_LINT_TO_WORD"/>
	</CompilerInfo>
	<InterfaceList>
		<Plugs>
			<AdapterDeclaration Name="AW_OUT" Type="adapter::types::unidirectional::AW" Comment="WORD Adapter Output" x="2000" y="0"/>
		</Plugs>
		<Sockets>
			<AdapterDeclaration Name="ALI_IN" Type="adapter::types::unidirectional::ALI" Comment="LINT Adapter Input" x="0" y="0"/>
		</Sockets>
	</InterfaceList>
	<FBNetwork>
		<FB Name="Convert" Type="iec61131::conversion::F_LINT_TO_WORD" x="1000" y="0">
		</FB>
		<EventConnections>
			<Connection Source="ALI_IN.E1" Destination="Convert.REQ" dx1="100"/>
			<Connection Source="Convert.CNF" Destination="AW_OUT.E1" dx1="100"/>
		</EventConnections>
		<DataConnections>
			<Connection Source="ALI_IN.D1" Destination="Convert.IN" dx1="100"/>
			<Connection Source="Convert.OUT" Destination="AW_OUT.D1" dx1="100"/>
		</DataConnections>
	</FBNetwork>
	<Attribute Name="eclipse4diac::core::TypeHash" Value="''"/>
</FBType>
