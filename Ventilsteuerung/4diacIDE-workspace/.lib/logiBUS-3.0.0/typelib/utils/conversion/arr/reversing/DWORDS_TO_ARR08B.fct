	<FunctionBody>
		<ST><![CDATA[PACKAGE logiBUS::utils::conversion::arr::reversing;

(* this Function combines 2 DWORDSs to a 8-BYTE Array; Little-Endian-Format (Intel-Standard) *)
FUNCTION DWORDS_TO_ARR08B

VAR_INPUT
	IN_00 : DWORD; // DWORD 00
	IN_01 : DWORD; // DWORD 01
END_VAR

VAR_OUTPUT
	OUT : ARRAY[0..7] OF BYTE; // ARR08B Output
END_VAR

OUT[0] := IN_00.%B0;
OUT[1] := IN_00.%B1;
OUT[2] := IN_00.%B2;
OUT[3] := IN_00.%B3;
OUT[4] := IN_01.%B0;
OUT[5] := IN_01.%B1;
OUT[6] := IN_01.%B2;
OUT[7] := IN_01.%B3;

END_FUNCTION
]]></ST>
	</FunctionBody>
