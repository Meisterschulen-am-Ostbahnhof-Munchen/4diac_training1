	<FunctionBody>
		<ST><![CDATA[PACKAGE logiBUS::utils::conversion::arr::forwarding;

(* this Function extracts the 4 WORD from a 8-BYTE Array *)
FUNCTION ARR08B_TO_WORDS

VAR_INPUT
	IN : ARRAY[0..7] OF BYTE; // ARR08B Input
END_VAR

VAR_OUTPUT
	OUT_00 : WORD; // WORD 00
	OUT_01 : WORD; // WORD 01
	OUT_02 : WORD; // WORD 02
	OUT_03 : WORD; // WORD 03
END_VAR

OUT_00.%B0 := IN[0];
OUT_00.%B1 := IN[1];
OUT_01.%B0 := IN[2];
OUT_01.%B1 := IN[3];
OUT_02.%B0 := IN[4];
OUT_02.%B1 := IN[5];
OUT_03.%B0 := IN[6];
OUT_03.%B1 := IN[7];

END_FUNCTION
]]></ST>
