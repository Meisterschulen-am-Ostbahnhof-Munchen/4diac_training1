<SubAppType Name="Uebung_091" Comment="Beispiel für E_TRAIN">
	<Identification Standard="61499-2" Description="Copyright (c) 2023 Meisterschulen am Ostbahnhof  &#10; &#10;This program and the accompanying materials are made  &#10;available under the terms of the Eclipse Public License 2.0  &#10;which is available at https://www.eclipse.org/legal/epl-2.0/  &#10; &#10;SPDX-License-Identifier: EPL-2.0">
	</Identification>
	<VersionInfo Organization="Meisterschulen am Ostbahnhof" Version="1.0" Author="Franz Höpfinger" Date="2023-10-23">
	</VersionInfo>
	<CompilerInfo>
		<Import declaration="logiBUS::io::DI::logiBUS_DI"/>
		<Import declaration="logiBUS::io::DQ::logiBUS_DO"/>
	</CompilerInfo>
	<SubAppInterfaceList>
	</SubAppInterfaceList>
	<SubAppNetwork>
		<FB Name="DigitalOutput_Q1" Type="logiBUS::io::DQ::logiBUS_QX" x="-200" y="-300">
			<Parameter Name="QI" Value="TRUE"/>
			<Parameter Name="Output" Value="logiBUS_DO::Output_Q1"/>
		</FB>
		<FB Name="E_TRAIN" Type="iec61499::events::E_TRAIN" x="-3100" y="-200">
			<Parameter Name="DT" Value="T#1s"/>
			<Parameter Name="N" Value="8"/>
		</FB>
		<FB Name="DigitalInput_CLK_I1" Type="logiBUS_IE" x="-4700" y="-400">
			<Parameter Name="QI" Value="TRUE"/>
			<Parameter Name="Input" Value="logiBUS_DI::Input_I1"/>
			<Parameter Name="InputEvent" Value="logiBUS_DI_Events::BUTTON_SINGLE_CLICK"/>
		</FB>
		<FB Name="E_T_FF" Type="iec61499::events::E_T_FF" x="-1800" y="-200">
		</FB>
		<Comment Comment="E_TRAIN wird 8 Events ausgeben, das erste 1s nach dem Click, das letzte 8s nach dem Click" x="-3200" y="600" width="1400" height="600"/>
		<EventConnections>
			<Connection Source="DigitalInput_CLK_I1.IND" Destination="E_TRAIN.START"/>
			<Connection Source="E_TRAIN.EO" Destination="E_T_FF.CLK"/>
			<Connection Source="E_T_FF.EO" Destination="DigitalOutput_Q1.REQ"/>
		</EventConnections>
		<DataConnections>
			<Connection Source="E_T_FF.Q" Destination="DigitalOutput_Q1.OUT" dx1="520"/>
		</DataConnections>
	</SubAppNetwork>
</SubAppType>
