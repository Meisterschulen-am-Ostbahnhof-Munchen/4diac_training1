	<FunctionBody>
		<ST><![CDATA[PACKAGE eclipse4diac::utils::conversion::arr::forwarding;

FUNCTION ARR08B_TO_BYTES

VAR_INPUT
	IN : ARRAY[0..7] OF BYTE; // ARR08B Input
END_VAR

VAR_OUTPUT
	OUT_00 : BYTE; // BYTE 00
	OUT_01 : BYTE; // BYTE 01
	OUT_02 : BYTE; // BYTE 02
	OUT_03 : BYTE; // BYTE 03
	OUT_04 : BYTE; // BYTE 04
	OUT_05 : BYTE; // BYTE 05
	OUT_06 : BYTE; // BYTE 06
	OUT_07 : BYTE; // BYTE 07
END_VAR

(* this Function extracts the 8 BYTE from a 8-BYTE Array *)
OUT_00 := IN[0];
OUT_01 := IN[1];
OUT_02 := IN[2];
OUT_03 := IN[3];
OUT_04 := IN[4];
OUT_05 := IN[5];
OUT_06 := IN[6];
OUT_07 := IN[7];

END_FUNCTION
