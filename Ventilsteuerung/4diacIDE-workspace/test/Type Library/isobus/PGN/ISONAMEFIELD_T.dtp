<?xml version="1.0" encoding="UTF-8"?>
<DataType Name="ISONAMEFIELD_T" Comment="ISO Part 5 name field structure according to IFM conventions">
	<Identification>
	</Identification>
	<VersionInfo Version="1.0" Author="Coding-Assistent" Date="2025-09-04">
	</VersionInfo>
	<CompilerInfo>
	</CompilerInfo>
	<StructuredType>
		<VarDeclaration Name="bSelfConf" Type="BOOL" Comment="Self-configurable-address ECU?" InitialValue="TRUE"/>
		<VarDeclaration Name="bIndGroup" Type="BYTE" Comment="NAMES associated with industries (0..7, 255 = Filter for all)" InitialValue="16#FF"/>
		<VarDeclaration Name="bDevClassInst" Type="BYTE" Comment="Identifier if a device class more than 1 times in net (0..15, 255 = Filter for all)" InitialValue="16#FF"/>
		<VarDeclaration Name="bDevClass" Type="BYTE" Comment="ISO Defined (0..127, 255 = Filter for all)" InitialValue="16#FF"/>
		<VarDeclaration Name="bReserved" Type="BOOL" Comment="Reserved (not used)" InitialValue="TRUE"/>
		<VarDeclaration Name="bFunction" Type="WORD" Comment="ISO defined, -&gt; SA (0..255, 65535 = Filter for all)" InitialValue="16#FFFF"/>
		<VarDeclaration Name="bFunctionInst" Type="BYTE" Comment="Subgroup of function (0..31, 255 = Filter for all)" InitialValue="16#FF"/>
		<VarDeclaration Name="bEcuInstance" Type="BYTE" Comment="Which ECU associated with a given function (0..7, 255 = Filter for all)" InitialValue="16#FF"/>
		<VarDeclaration Name="wManufCode" Type="WORD" Comment="ISO defined (0..2047, 65535 = Filter for all)" InitialValue="16#FFFF"/>
		<VarDeclaration Name="dwIdentNumb" Type="DWORD" Comment="Used when the NAME is not unique in same network (0..2047, 4294967295 = Filter for all)" InitialValue="16#FFFFFFFF"/>
	</StructuredType>
</DataType>
