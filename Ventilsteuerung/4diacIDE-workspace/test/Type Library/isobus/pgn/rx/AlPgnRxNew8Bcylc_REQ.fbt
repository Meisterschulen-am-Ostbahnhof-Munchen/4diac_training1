<?xml version="1.0" encoding="UTF-8"?>
<FBType Name="AlPgnRxNew8Bcylc_REQ" Comment="Request data cyclic">
	<Identification Standard="61499-2">
	</Identification>
	<VersionInfo Version="1.0" Author="franz" Date="2025-09-04">
	</VersionInfo>
	<CompilerInfo packageName="isobus::pgn::rx">
		<Import declaration="isobus::pgn::CAN_MSG"/>
		<Import declaration="isobus::pgn::ISONETEVENT_T"/>
	</CompilerInfo>
	<InterfaceList>
		<EventInputs>
			<Event Name="INIT" Type="EInit" Comment="Service Initialization">
			</Event>
			<Event Name="install" Type="Event" Comment="installs a Transmit PGN (TX) ">
				<With Var="u32Pgn"/>
				<With Var="NmSource"/>
				<With Var="u16DaSize"/>
				<With Var="u8Priority"/>
				<With Var="u16DefRepRate"/>
				<With Var="u16CtrlTime"/>
			</Event>
		</EventInputs>
		<EventOutputs>
			<Event Name="INITO" Type="EInit" Comment="Initialization Confirm">
			</Event>
			<Event Name="installO" Type="Event" Comment="install Done">
				<With Var="PGN_handle"/>
			</Event>
			<Event Name="IND" Type="Event" Comment="Data was received">
				<With Var="bTimeout"/>
				<With Var="s32TimeStamp"/>
				<With Var="Data"/>
			</Event>
			<Event Name="TIMEOUT" Type="Event">
				<With Var="bTimeout"/>
				<With Var="s32TimeStamp_timeout"/>
			</Event>
			<Event Name="dataERR" Type="Event">
				<With Var="dataERRC"/>
			</Event>
			<Event Name="pgnERR" Type="Event">
				<With Var="pgnERRC"/>
			</Event>
		</EventOutputs>
		<InputVars>
			<VarDeclaration Name="u32Pgn" Type="UDINT" Comment="PGN - Range: 0 - 0x3FFFF "/>
			<VarDeclaration Name="NmSource" Type="isobus::pgn::ISONETEVENT_T" Comment="Define the Communication Partner"/>
			<VarDeclaration Name="u16DaSize" Type="UINT" Comment="Data length of this PGN (0..8)"/>
			<VarDeclaration Name="u8Priority" Type="USINT" Comment="Default priority of this PGN (0..7) 0=highest" InitialValue="7"/>
			<VarDeclaration Name="u16DefRepRate" Type="UINT" Comment="0 ... 0xFDFF ms for time interval of cyclic transmission"/>
			<VarDeclaration Name="u16CtrlTime" Type="UINT" Comment="0 ... 0xFDFF ms Control time of a receive PGN"/>
		</InputVars>
		<OutputVars>
			<VarDeclaration Name="PGN_handle" Type="INT" Comment="PGN-handle on success, HANDLE_UNVALID in case of an error "/>
			<VarDeclaration Name="dataERRC" Type="INT" Comment="dataERR error Code"/>
			<VarDeclaration Name="pgnERRC" Type="INT" Comment="pgnERR error Code"/>
			<VarDeclaration Name="bTimeout" Type="BOOL" Comment="Timeout occured TRUE = Timeout, FALSE = no Timeout" InitialValue="FALSE"/>
			<VarDeclaration Name="s32TimeStamp_timeout" Type="DINT" Comment="Time stamp of the Timeout in ms"/>
			<VarDeclaration Name="s32TimeStamp" Type="DINT" Comment="Time stamp of the message in ms" InitialValue="-1"/>
			<VarDeclaration Name="Data" Type="isobus::pgn::CAN_MSG" Comment="data buffer"/>
		</OutputVars>
	</InterfaceList>
</FBType>
