<?xml version="1.0" encoding="UTF-8"?>
<FBType Name="E_SEL_2" Comment="event-driven selection of data">
	<Identification Standard="61499-2" Description="An instance of an E_SEL_2 FB type provides event-driven selection of data between two inputs. Data at the OUT variable is copied from the data ( IN0 or IN1, respectively) associated with the most recent event at the EI0 or EI1 input, followed by an event at the EO output.&#10;&#10;This work is inspired by https://holobloc.com/doc/fb/rt/select/E_SEL_xxx.htm &#10;" >
	</Identification>
	<VersionInfo Version="1.0" Author="franz" Date="2024-07-03">
	</VersionInfo>
	<CompilerInfo>
	</CompilerInfo>
	<InterfaceList>
		<EventInputs>
			<Event Name="REQ0" Type="Event" Comment="">
				<With Var="IN0"/>
			</Event>
			<Event Name="REQ1" Type="Event" Comment="">
				<With Var="IN1"/>
			</Event>
		</EventInputs>
		<EventOutputs>
			<Event Name="EO" Type="Event" Comment="">
				<With Var="OUT"/>
			</Event>
		</EventOutputs>
		<InputVars>
			<VarDeclaration Name="IN0" Type="ANY" Comment=""/>
			<VarDeclaration Name="IN1" Type="ANY" Comment=""/>
		</InputVars>
		<OutputVars>
			<VarDeclaration Name="OUT" Type="ANY" Comment=""/>
		</OutputVars>
	</InterfaceList>
	<BasicFB>
		<ECC>
			<ECState Name="START" Comment="Initial State" x="403.33" y="805">
			</ECState>
			<ECState Name="COPY0" Comment="" x="1333.33" y="533.33">
				<ECAction Algorithm="COPY0" Output="EO"/>
			</ECState>
			<ECState Name="COPY1" Comment="" x="1333.33" y="1200">
				<ECAction Algorithm="COPY1" Output="EO"/>
			</ECState>
			<ECTransition Source="START" Destination="COPY0" Condition="REQ0" Comment="" x="1000" y="766.67"/>
			<ECTransition Source="START" Destination="COPY1" Condition="REQ1" Comment="" x="1013.33" y="1093.33"/>
			<ECTransition Source="COPY0" Destination="START" Condition="1" Comment="" x="600" y="420"/>
			<ECTransition Source="COPY1" Destination="START" Condition="1" Comment="" x="660" y="1473.33"/>
		</ECC>
		<Algorithm Name="COPY0" Comment="">
			<ST><![CDATA[ALGORITHM COPY0
OUT := IN0;
END_ALGORITHM]]></ST>
		</Algorithm>
		<Algorithm Name="COPY1" Comment="">
			<ST><![CDATA[

ALGORITHM COPY1
OUT := IN1;
END_ALGORITHM

]]></ST>
		</Algorithm>
	</BasicFB>
</FBType>
