	<InterfaceList>
		<EventInputs>
			<Event Name="INIT" Type="EInit" Comment="Service Initialization">
				<With Var="QI"/>
				<With Var="u8SAMember"/>
			</Event>
		</EventInputs>
		<EventOutputs>
