		<ST><![CDATA[PACKAGE logiBUS::utils::conversion::str::converting;

IMPORT logiBUS::utils::conversion::types::ST08B;

(* this Function takes a a 8-BYTE Array and makes a 8-BYTE Struct *)
FUNCTION ARR08B_TO_ST08B

VAR_INPUT
	IN : ARRAY[0..7] OF BYTE; // ARR08B Input
END_VAR

VAR_OUTPUT
	OUT : ST08B; // ST08B Output
END_VAR

OUT.B_00 := IN[0];
OUT.B_01 := IN[1];
OUT.B_02 := IN[2];
OUT.B_03 := IN[3];
OUT.B_04 := IN[4];
OUT.B_05 := IN[5];
OUT.B_06 := IN[6];
OUT.B_07 := IN[7];

END_FUNCTION
