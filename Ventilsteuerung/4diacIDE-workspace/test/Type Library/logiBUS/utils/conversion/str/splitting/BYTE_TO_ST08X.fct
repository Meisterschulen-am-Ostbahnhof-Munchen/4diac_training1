	<FunctionBody>
		<ST><![CDATA[PACKAGE logiBUS::utils::conversion::str::splitting;

IMPORT logiBUS::utils::conversion::types::ST08X;

(* this Function extracts the 8 BOOLS from a byte and make a 8-BOOL Struct *)
FUNCTION BYTE_TO_ST08X

VAR_INPUT
	IN : BYTE; // BYTE Input
END_VAR

VAR_OUTPUT
	OUT : ST08X; // ST08X Output
END_VAR

OUT.X_00 := IN.0;
OUT.X_01 := IN.1;
OUT.X_02 := IN.2;
OUT.X_03 := IN.3;
OUT.X_04 := IN.4;
OUT.X_05 := IN.5;
OUT.X_06 := IN.6;
OUT.X_07 := IN.7;

END_FUNCTION
]]></ST>
