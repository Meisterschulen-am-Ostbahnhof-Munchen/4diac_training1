		<ST><![CDATA[PACKAGE logiBUS::utils::conversion::str::converting;

IMPORT logiBUS::utils::conversion::types::ST08B;

(* this Function takes a a 8-BYTE Struct and makes a 8-BYTE Array *)
FUNCTION ST08B_TO_ARR08B

VAR_INPUT
	IN : ST08B; // ST08B Input
END_VAR

VAR_OUTPUT
	OUT : ARRAY[0..7] OF BYTE; // ARR08X Output
END_VAR

OUT[0] := IN.B_00;
OUT[1] := IN.B_01;
OUT[2] := IN.B_02;
OUT[3] := IN.B_03;
OUT[4] := IN.B_04;
OUT[5] := IN.B_05;
OUT[6] := IN.B_06;
OUT[7] := IN.B_07;

END_FUNCTION
]]></ST>
